<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Add">
        <Item>
          <IdeaActionID>Git.Add</IdeaActionID>
          <ShortCut>'⌥⌘A'</ShortCut>
          <Description>Add</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>32</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Fetch">
        <Item>
          <IdeaActionID>Git.Fetch</IdeaActionID>
          <ShortCut>'⌥F'</ShortCut>
          <Description>Fetch</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'⌥F7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find in Path...">
        <Item>
          <IdeaActionID>FindInPath</IdeaActionID>
          <ShortCut>'⇧⌘F'</ShortCut>
          <Description>Find in Path...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Move...">
        <Item>
          <IdeaActionID>Move</IdeaActionID>
          <ShortCut>'F6'</ShortCut>
          <Description>Move...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Preferences...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'⌘,'</ShortCut>
          <Description>Preferences...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'⇧F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Start Listening for PHP Debug Connections">
        <Item>
          <IdeaActionID>PhpListenDebugAction</IdeaActionID>
          <ShortCut>'⌃⌥⌘D'</ShortCut>
          <Description>Start Listening for PHP Debug Connections</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop Listening for PHP Debug Connections">
        <Item>
          <IdeaActionID>PhpListenDebugAction</IdeaActionID>
          <ShortCut>'⌃⌥⌘D'</ShortCut>
          <Description>Stop Listening for PHP Debug Connections</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌥F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'⌘C'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste">
        <Item>
          <IdeaActionID>$Paste</IdeaActionID>
          <ShortCut>'⌘V'</ShortCut>
          <Description>Paste</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>